{
    "compilerOptions": {
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "outDir": "./dist/main",
      "module": "commonjs",
      "target": "ESNext",
      "types": [
        "node",
        "./types"
      ],
      "lib": [
        "es2017",
        "es2016",
        "es2015",
        "dom"
      ]
    },
    "exclude": [
      "node_modules",
      "**/*.spec.ts",
      "dist",
      "resources",
      "compiled"
    ]
  }
  